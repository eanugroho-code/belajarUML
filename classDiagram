@startuml
class person{
-nama : string
-age : int
+getName() :string
+getAge() : int
}

class teacher{
-employeID : string
+getemployID() :string
}

class student{
-studentID : string
+getstudentID : string
}

person <|-- student
person <|-- teacher


class manusia{
Nama Person
Umur Person
}

class guru{
-id001
+EKO ARI NUGROHO
+29 TAHUN
}

class siswa{
-ids001
+kak Ely
-ids002
+kak pieter
}

manusia <|-- siswa
manusia <|-- guru
@enduml
